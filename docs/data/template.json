{
  "nav": "{{#pages}}<li class=\"nav-item\"><a class=\"nav-link active\" href= \"#{{&link}}\">{{&name}}</a></li>{{/pages}}",
  "tab": "{{#pages}}<div class=\"tab-pane fade\" id=\"{{&id}}\"></div>{{/pages}}",
  "container": "<div class=\"container\" id='container'>\n<h4>Icons</h4>\n<p>Icon list to search from.</p>\n\n<ul id=\"mlist\" class=\"nav nav-tabs\">\n  <li class=\"nav-item\">\n    <a class=\"nav-link active\" data-toggle=\"tab\" href=\"#home\">Home</a>\n  </li>\n    {{&list}}\n</ul>\n\n<div id=\"tabs\" class=\"tab-content\">\n  <div class=\"tab-pane fade active show\" id=\"home\">\n    <p>Home</p>\n  </div>\n  {{&tab}}\n</div>",
  "dropdown": " <div id='{{id}}' class=\"dropdown-menu\">\n{{#items}}\n<a class=\"dropdown-item\" href=\"#\">{{.}}</a>\n{{/items}}\n</div>",
  "select": "{{#data}}\n<div id=\"{{g_id}}\" class=\"col\">\n<label for=\"{{id}}\">{{name}}</label>\n<select id=\"{{id}}\" class=\"form-control\">\n{{#values}}\n<option>{{.}}</option>\n{{/values}}\n</select>\n</div>\n{{/data}}",
  "colors": {
    "dropdown": "{{#values}}\n<a class=\"dropdown-item\" href=\"#\" style=\"background-color: {{.}}\">{{.}}</a>\n{{/values}}",
    "div": "{{#data}}\n<label for='{{id}}'>{{label}}</label>\n<div id='{{id}}' class='input-group mb-2 mr-sm-2'>\n<span class='input-group-prepend'>\n  <button class='btn btn-outline-dark dropdown-toggle' type='button' data-toggle='dropdown'>Input</button>\n  <span id='{{id}}_ip' class='dropdown-menu'>\n  </span>\n</span>\n  <input id='{{id}}_text' type='text' class='form-control input-sm' value='{{value}}'/>\n  <span class='input-group-append'>\n  <span class='input-group-text colorpicker-input-addon'><i></i></span>\n  </span>\n</div>\n{{/data}}"
  }
}
