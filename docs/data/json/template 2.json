{
  "icon": "<a tabindex=\"0\" role=\"button\" class=\"btn btn-light px-2\" data-type=\"icon\" data-trigger=\"click focus\" data-name=\"{{name}}\" style=\"background-color:#FFFFFF; color: black;\"><i class='{{prefix}} fa-{{name}}  fa-2x fa-fw'></i></a>",
  "terms": "{{#terms}}<a class=\"badge rgba-blue-light dark-text shadow-none\">{{.}}</a> {{/terms}}",
  "dom": "{{#escape}}<i class='{{&prefix}} fa-{{&name}}'></i>{{/escape}}",
  "detail": "{{#detail}}\n{{key}}: {{&value}}</br>\n{{/detail}}",
  "prefixes": "{{#prefixes}}\n{{style}} ({{prefix}})\n{{/prefixes}}",
  "unicode": "<code>{{unicode}}</code>",
  "pills": "{{#values}}<a class=\"badge mr-1 mb-1 waves-effect\" herf=\"#\" value=\"{{.}}\" style=\"background: {{.}};\">{{.}}</a>{{/values}}",
  "tbtoolbar": "{{#list}}<button type=\"button\" data-input=\"{{input}}\" id=\"{{input}}\" {{#toggle}}data-toggle=\"dropdown\"{{/toggle}}class=\"btn  btn-md btn-outline rgba-purple-slight  waves-effect\">{{#icon}}<span class='fas fa-{{icon}}'></span>{{/icon}}</button>{{#toggle}} <div class=\"dropdown-menu\" aria-labelledby=\"{{input}}\"></div>{{/toggle}}{{/list}}",
  "dpmenu": " <label class=\"dropdown-item dropdown-item-marker\">\n<input type=\"checkbox\" data-field=\"all\" class=\"toggle-all\"> <span>Toggle all</span></label>\n<div class=\"dropdown-divider\"></div>\n{{#list}}\n<label class=\"dropdown-item dropdown-item-marker\">\n<input type=\"checkbox\" data-field=\"{{field}}\" value=\"1\" {{#visible}} checked=\"checked\"{{/visible}}> <span>{{field}}</span></label>\n{{/list}}",
  "cldp": "{{#list}}\n{{#header}} <h5 class=\"dropdown-header\">\n{{#icon}}<span class='fas fa-{{icon}}'></span>{{/icon}} {{value}}</h5>\n{{/header}}\n{{^header}}\n<a class=\"dropdown-item\" data-input=\"{{input}}\" value=\"{{value}}\"> {{#icon}}<span class='fas fa-{{icon}}'></span> {{/icon}}{{value}}</a>\n{{/header}}\n{{/list}}"
}
